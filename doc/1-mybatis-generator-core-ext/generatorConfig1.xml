<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
   <!--  <classPathEntry    location="mysql-connector-java-5.1.26.jar"/> -->
    <!-- <properties resource="application.properties"/> -->
    <context id="DB2Tables"    targetRuntime="MyBatis3">
   
    <plugin type="com.zichan100.biz.ExtImplPlugin"></plugin>
		<!--去除注释  -->
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
       
	    <!-- jdbc连接信息 -->
         <jdbcConnection driverClass="com.mysql.jdbc.Driver" 
		connectionURL="jdbc:mysql://zc100testo.mysql.rds.aliyuncs.com:3306/dds_dev?characterEncoding=UTF-8" userId="dbowner" password="!!!abc123">
        </jdbcConnection>
		
     <!-- 生成bean对象 --> 
	    <!-- targetProject:指定目标项目路径，使用的是文件系统的绝对路径。 --> 
        <javaModelGenerator targetPackage="com.zichan100.demo.pojo" targetProject="src">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="false"/>
        </javaModelGenerator>
		
		<!-- 生成用于查询的 xml文件 对象 -->  
        <sqlMapGenerator targetPackage="com.zichan100.demo.mapper.base" targetProject="src">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="false"/>
            <property name="rootClass" value="BaseDaoInf"/>
        </sqlMapGenerator>
        
		<!-- 生成DAO的类文件以及配置文件 java类型 -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.zichan100.demo.mapper.base" targetProject="src">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>
	
		
		<table tableName="demo_test1" domainObjectName="DemoTest1" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
			<!-- 命名规范：使用驼峰命名法 -->
			<property name="useActualColumnNames" value="true"/>
		</table>
		
		
    </context>
</generatorConfiguration>
