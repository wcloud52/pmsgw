<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
	updateCheck="false" name="bizServiceCache">

	<diskStore path="java.io.tmpdir/ehcache/bizService" />

	<!-- DefaultCache setting. -->
	<defaultCache eternal="false" maxElementsInMemory="1000"
		overflowToDisk="true" diskPersistent="false" timeToIdleSeconds="0"
		timeToLiveSeconds="3600" memoryStoreEvictionPolicy="LRU" />

		
		<cache name="bizServiceCache" eternal="false" maxElementsInMemory="1000" 
	overflowToDisk="true" diskPersistent="false"
		timeToIdleSeconds="0" timeToLiveSeconds="3600"
		memoryStoreEvictionPolicy="LRU" />
</ehcache>