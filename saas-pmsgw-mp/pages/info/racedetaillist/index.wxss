.load-btn{
  margin-left: 8px;
  margin-right: 8px;
  margin-top: 16px;
  margin-bottom: 16px;
  display: block;
  line-height: 2.3;
  font-size: 13px;
  padding: 0 1.34em;
}
.title{
  text-align: center;
  padding: 40rpx 0 20rpx 0;
  font-size: 36rpx;
  font-weight: bold;
}
/*racelist*/
.left-title{
  padding: 10rpx 30rpx;
  font-size: 30rpx;
  font-weight: bold;
}
.search{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx;
}
.selcet-search{
  padding-left: 30rpx;
}
.search-input-wrap{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  background: #fff;
  width: 608rpx;
  height: 68rpx;
  border-radius: 34rpx; 
}
.select-wrap{
  border-radius: 0;
}
.select-wrap  .search-input{
  padding-left: 20rpx;
}
.search-icon{
  width: 48rpx;
  height: 48rpx;
  margin: 0 15rpx;
}
.search-input{
  width: 452rpx;
  height: 100%;
}
.input-placeholder{
  color: #999;
}
.search-btn{
  color: #018AFE;
  padding-right: 20rpx;
}

page {
  background: #f5f5f5;
  font-size: 28rpx;
  color: #333333;
  min-height: 100vh;
}

.table-wrap{
  background: #fff;
  margin: 10rpx 30rpx;
  width: calc(100vw - 60rpx);
}
.table{
 width: 1830rpx;
 border:1px solid #dadada;
  border-right: 0;
  border-bottom: 0;
}
.th,.td {
  padding: 5px;
  border-bottom: 1px solid #dadada;
  border-right: 1px solid #dadada;
  text-align: left;
  width:100%
}
.td-rank {
   width: 350rpx;
}
.race-time
{
   font-size: 10px;
   color: #f10e0e;
   text-align: right;
}
.tr {
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.th {
  font-weight: 400;
  background-color: #dadada
} 


