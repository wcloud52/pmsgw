  padding-left: 20rpx;
}
.search-icon{
  width: 48rpx;
  height: 48rpx;
  margin: 0 15rpx;
}
.search-input{
  width: 452rpx;
  height: 100%;
}
.input-placeholder{
  color: #999;
}
.search-btn{
  color: #018AFE;
  padding-right: 20rpx;
}

